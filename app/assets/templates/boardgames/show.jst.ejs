<h1><%= boardgame.get('title') %></h1>
<%= console.log(boardgame.get('image')) %>
<div style="width:300px;height:300px" >
   <img src="<%= boardgame.get('image') %>" width="auto" height="300px">
</div>
Rating: <div id="star"></div><%= boardgame.get('rating') %>
<br>
<%= boardgame.get('description') %>

<a href="#boardgames/<%= boardgame.get('id') %>/reviews/new">Review this Game</a>
<h1>Reviews</h1>
<ul>
	<%reviews.each(function(review) { %>
		<li class="embedded" id="<%=review.get('id')%>"></li>
	<% }) %>
</ul>
